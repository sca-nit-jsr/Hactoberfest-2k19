#include <stdio.h>

int main(){
	int t,n,i,j;
	scanf("%d",&t);
	while(t--)
	{
	    scanf("%d",&n);
	    for(i=1;i<=n;i++)
	    {
	        for(j=1;j<=i;j++)
	            printf("*");
	        for(j=1;j<=((n-i)*2);j++)
	            printf("#");
	        for(j=1;j<=i;j++)
	            printf("*");
	        printf("\n");
	    }
	}
	return 0;
}
